{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDirs": [
      "./",
      "./_commons_"
    ],
    "baseUrl": "./",
    "paths": {
      "commons/*": [
        "../_commons_/*"
      ]
    },
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}